    "@wordpress/components": "^29.6.0",
    "@wordpress/element": "^6.2.0",
    "@wordpress/scripts": "^26.19.0",
    "antd": "^5.24.3",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "glob": "^10.4.5",
    "handlebars": "^4.7.8",
    "rimraf": "^6.0.1",
    "yargs": "^17.7.2"
